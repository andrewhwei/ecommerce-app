<h1>Products</h1>

<table class="table table-striped">
  <th>Name</th>
  <th>Price</th>
  <th>Image</th>
  <th>Description</th>
  
  <% @products.each do |product| %>
    <tr>
      <td><%= product.name %></td>
      <td><%= product.price %></td>
      <td><%= product.image %></td>
      <td><%= product.description %></td>
    </tr>
  <% end %>

</table>